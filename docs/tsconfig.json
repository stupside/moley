{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "preserve",
		"jsxImportSource": "astro",
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"strict": true,
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"resolveJsonModule": true
	}
}
